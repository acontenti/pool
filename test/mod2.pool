"\u250C\n\u2502 Called from "
    .+(file)
    .+(" with args:\n\u2502\t")
	.+(args)
	.+("\n\u2514")
	.println();

vtest := (a1, b1, ...c): {
	a1.+(b1).println();
	c.println();
};
vtest(1, 2);
vtest(10, 2, 3, 4, 5, 6);
vargs := [2, 3, 4, 5, 6, 7, 8];
vtest(100, 200, ...vargs, 3000);
[...vargs].println();
vtest(...[0, 1, 2], ...[...vargs]);
vtest2 := (a1, b1): {
	a1.-(b1).println();
};
vtest2(1, 2);